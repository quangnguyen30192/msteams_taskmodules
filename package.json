{
  "name": "youtubeplayer",
  "version": "0.0.1",
  "description": "Generated by yoteams, https://aka.ms/yoteams",
  "scripts": {
    "start": "node dist/server.js",
    "build": "gulp build",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --quiet --fix",
    "lint-css": "stylelint src/**/*.scss",
    "debug": "gulp serve --debug",
    "serverok": "gulp ngrok-serve",
    "serve": "gulp serve",
    "prettier:write": "prettier --parser typescript --write \"./src/**/*.{ts,tsx}\"",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "test": "jest --passWithNoTests",
    "type:check": "yarn tsc --noEmit",
    "gulp:tasks": "gulp --tasks"
  },
  "dependencies": {
    "@fluentui/react-northstar": "~0.52.0",
    "@microsoft/teams-js": "~1.8.0",
    "axios": "^0.21.1",
    "botbuilder": "4.11.1",
    "botbuilder-dialogs": "4.11.1",
    "compression": "1.7.4",
    "debug": "4.2.0",
    "dotenv": "8.2.0",
    "express": "4.16.4",
    "express-msteams-host": "1.7.0",
    "express-session": "1.15.6",
    "morgan": "1.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "msteams-react-base-component": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier:write"
    ],
    "*.{css,scss}": [
      "yarn prettier:write"
    ],
    "*.{md,html,json}": [
      "yarn prettier:write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "testRegex": "src/.*(\\.(test|spec))\\.(tsx?)$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/config/tests/setup.ts"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageReporters": [
      "lcov"
    ]
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/express": "4.16.0",
    "@types/express-session": "1.15.10",
    "@types/morgan": "1.7.35",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "browser-sync": "^2.26.5",
    "dotenv-webpack": "^3.0.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.0",
    "fancy-log": "^1.3.3",
    "file-loader": "6.1.1",
    "gulp": "4.0.2",
    "ngrok": "^3.1.1",
    "ts-loader": "8.0.5",
    "typescript": "4.0.3",
    "typestyle": "2.0.1",
    "vinyl": "2.2.0",
    "webpack": "5.0.0",
    "yargs": "^16.0.3",
    "yoteams-build-core": "^1.0.0",
    "webpack-node-externals": "^2.5.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ]
}
